generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String   @id @default(cuid())
  email          String   @unique
  name           String
  phone          String?
  password       String
  tradingAmount  String?
  referralId     String?
  experience     String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
